{
    "name": "@stellar-keystone/cli",
    "version": "1.0.0",
    "description": "CLI for Stellar Keystone RBAC",
    "main": "dist/index.js",
    "bin": {
        "rbac": "./dist/index.js"
    },
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node --esm src/index.ts",
        "test": "jest",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write src/**/*.ts"
    },
    "keywords": [
        "stellar",
        "soroban",
        "rbac",
        "cli"
    ],
    "author": "Stellar Keystone Contributors",
    "license": "MIT",
    "dependencies": {
        "@stellar-keystone/sdk": "file:../js-sdk",
        "@stellar/stellar-sdk": "^12.0.0",
        "better-sqlite3": "^12.6.2",
        "commander": "^12.0.0"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
